{{extend 'layout.html'}}
<h1>Shopping Cart</h1>
<p>
    The item was added to your cart.
</p>

<table class='table table-striped table-hover'>
    {{ for x in rows: }}
    <tr>
        <td>{{=x.qty}}</td>
        <td>{{=prodDict[x.productId]}}</td>
        <td><a href='delete'>Delete</a></td>
    </tr>
    {{pass}}
</table>
&nbsp;&nbsp;&nbsp;<a href='view'>Continue Shopping</a>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<button>Checkout</button>
